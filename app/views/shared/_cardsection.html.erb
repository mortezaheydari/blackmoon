<!-- remeber the doppelganger -->
<%= content_tag("div", "", class: "cardsection #{class_name}", remote: :true) do %>

    <%  individuals.each do |individual| %>
        <% if class_name == "likes" %>
            <%= render 'users/usercards', person: individual.flagger, event: event %>
        <% else %>
            <%= render 'users/usercards', person: individual, event: event %>
        <% end %>
    <% end %>
<% end %>